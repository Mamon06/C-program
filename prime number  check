#include <stdio.h>

int main() {
    int n,i,prime_number=1;
 printf("Enter the number:");
 scanf("%d",&n);
 for(i=2;i<n;i++)
 {
     if((n%i)==0)
     {
          prime_number=0;
     }
 }
  if(prime_number==1)
 {
     printf("Is a prime number");
 }
 else
 {
      printf("Is Not a prime number");
 }
    return 0;
}
