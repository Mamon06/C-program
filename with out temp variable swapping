# C-program
#include <stdio.h>

int main() {
     int n1,n2;
     printf("Enter 1st number:");
     scanf("%d",&n1);
     printf("Enter 2nd number:");
     scanf("%d",&n2);
     n1=n1-n2;
     n2=n1+n2;
     n1=n2-n1;
     printf(" After Swapping 1st number:%d\n",n1);
     printf(" After Swapping 2nd number:%d",n2);

    return 0;
}
