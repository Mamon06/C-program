#include <stdio.h>

int main() {
       int num1,num2;//initilize 1st and 2nd number
       printf("Enter the number:");
       scanf("%d",&num1);
       printf("Enter the number:");
       scanf("%d",&num2);
       int sum = num1+num2;
       printf("Sum of number:%d",sum);
   
    return 0;
}
